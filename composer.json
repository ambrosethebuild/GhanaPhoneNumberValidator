{
    "name": "ambrosethebuild/ghanaphonenumbervalidator",
    "description": ":A Laravel Package To Validate A Ghana Phone Number and Also Return the network it belongs to",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambrose Bako",
            "email": "bakoambrose@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "keywords": ["Laravel", "GhanaPhoneNumberValidator","Ghana Laravel"],
    "require": {
        "illuminate/support": "~5.1|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AmbroseTheBuild\\GhanaPhoneNumberValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmbroseTheBuild\\GhanaPhoneNumberValidator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmbroseTheBuild\\GhanaPhoneNumberValidator\\GhanaPhoneNumberValidatorServiceProvider"
            ],
            "aliases": {
                "GhanaPhoneNumberValidator": "AmbroseTheBuild\\GhanaPhoneNumberValidator\\Facades\\GhanaPhoneNumberValidator"
            }
        }
    }
}
